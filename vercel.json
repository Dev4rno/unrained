{
    "version": 2,
    "builds": [
        {
            "src": "unrained/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/js/script.js",
            "destination": "https://plausible.io/js/script.js"
        },
        {
            "source": "/api/event",
            "destination": "https://plausible.io/api/event"
        },
        {
            "source": "/(.*)",
            "destination": "/unrained/wsgi.py"
        }
    ]
}
